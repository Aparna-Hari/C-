@model List<Dish>

<h1> Welcome to CRUDDelicious!</h1>

<h3>Check out some dishes!</h3>

<a  asp-controller="Dishes" asp-action="New">Add a Dish</a>

@foreach (Dish dish in Model)
{
    <p> @dish.Name  by @dish.Chef <a  asp-controller="Dishes" asp-action = "Details", asp-route-dishId="@dish.DishId">ViewDetails</a></p>
}